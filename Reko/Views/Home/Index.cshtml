<div class="card">
    <div class="card-header">
        Featured
    </div>
    <div class="card-body">
        <h5 class="card-title">Dates</h5>
        <form action="@Url.Action("Run")" method="post">
            <div class="table">
                <div class="row">
                    <div class="col-6">
                        <input type="date" name="from" placeholder="From" />
                    </div>
                    <div class="col-6">
                        <input type="date" name="to" placeholder="To" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <input type="submit" class="btn btn-primary" value="Run" />
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
